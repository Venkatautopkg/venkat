<?xml version="1.0" encoding="UTF-8"?>
<BES xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="BES.xsd">
	<{{TypeTaskOrFixlet}}{{^TypeTaskOrFixlet}}Task{{/TypeTaskOrFixlet}}>
		<Title>{{^patch}}Install/{{/patch}}Update: {{DisplayName}} v{{version}} - Windows{{#64BitOnly}} (x64){{/64BitOnly}}{{#32BitOnly}} (x32){{/32BitOnly}}</Title>
		<Description><![CDATA[
{{>DescriptionBasic}}
{{>DescriptionIcon}}
		]]></Description>
		<Relevance>windows of operating system</Relevance>
		{{#64BitOnly}}<Relevance><![CDATA[x64 of operating system]]></Relevance>{{/64BitOnly}}
		{{#32BitOnly}}<Relevance><![CDATA[not x64 of operating system]]></Relevance>{{/32BitOnly}}
		{{#patch}}{{#DisplayName}}
		<Relevance><![CDATA[ exists keys whose (value "DisplayName" of it as string starts with "Sandboxie" AND it as string does not start with "Sandboxie-Plus") of keys "HKLM\Software\Microsoft\Windows\CurrentVersion\Uninstall" of (x32 registries; x64 registries) ]]></Relevance>
		{{/DisplayName}}{{/patch}}
		{{#PreRelevance}}<Relevance><![CDATA[{{{PreRelevance}}}]]></Relevance>{{/PreRelevance}}
		<Relevance><![CDATA[
not exists (it as string as version) whose(it >= "{{version}}") of values "DisplayVersion" of keys whose (value "DisplayName" of it as string starts with "Sandboxie" AND it as string does not start with "Sandboxie-Plus") of keys "HKLM\Software\Microsoft\Windows\CurrentVersion\Uninstall" of (x32 registries; x64 registries)
		]]></Relevance>
		<Category></Category>
		<DownloadSize>{{DownloadSize}}{{^DownloadSize}}0{{/DownloadSize}}</DownloadSize>
		<Source>{{VendorFolder}}</Source>
		<SourceID>BigFix</SourceID>
		<SourceReleaseDate>{{SourceReleaseDate}}</SourceReleaseDate>
		<SourceSeverity></SourceSeverity>
		<CVENames></CVENames>
		<SANSID></SANSID>
		<MIMEField>
			<Name>action-ui-metadata</Name>
			<Value>{{>action-ui-metadata}}</Value>
		</MIMEField>
		{{>cpe23}}
		<MIMEField>
			<Name>x-fixlet-modification-time</Name>
			<Value>{{x-fixlet-modification-time}}</Value>
		</MIMEField>
		{{#MIMEFields}}{{>MIMEField}}{{/MIMEFields}}
		<Domain>BESC</Domain>
		<DefaultAction ID="Action1">
			<Description>
				<PreLink><![CDATA[Click ]]></PreLink>
				<Link>here</Link>
				<PostLink><![CDATA[ to deploy {{DisplayName}} v{{version}}.]]></PostLink>
			</Description>
			<ActionScript MIMEType="application/x-Fixlet-Windows-Shell"><![CDATA[
// Download:
{{{prefetch}}}
{{{PreActionScript}}}
// Install:
override wait
hidden=true
completion=job
wait {{#exe_file}}__Download\{{{file_name}}}{{/exe_file}}{{^exe_file}}msiexec.exe /i __Download\{{{file_name}}} /qn /norestart{{/exe_file}}{{#cmd_args}} {{{cmd_args}}}{{/cmd_args}}
{{{PostActionScript}}}
{{#PublicDesktopShortcutFile}}{{> DeletePublicDesktopIcon}}{{/PublicDesktopShortcutFile}}
// End]]></ActionScript>
      		<SuccessCriteria Option="OriginalRelevance"></SuccessCriteria>
		</DefaultAction>
	</{{TypeTaskOrFixlet}}{{^TypeTaskOrFixlet}}Task{{/TypeTaskOrFixlet}}>
</BES>
